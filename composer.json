{
    "name": "acelot/automapper",
    "description": "Simple declarative data mapper for PHP 7",
    "keywords": ["mapper", "automapper", "data-mapper"],
    "type": "library",
    "homepage": "https://github.com/acelot/automapper",
    "license": "MIT",
    "authors": [
        {
            "name": "Valeriy Protopopov",
            "email": "provaleriy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Acelot\\AutoMapper\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Acelot\\AutoMapper\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml"
    }
}
